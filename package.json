  "license": "ISC",
  "dependencies": {
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.19",
    "zx": "^8.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.1",
    "kolorist": "^1.8.0",
    "ora": "^8.0.1",
    "standard-version": "^9.5.0",
    "vite": "^5.2.10",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-node-polyfills": "^0.21.0"
  },
  "config": {
    "commitizen": {
